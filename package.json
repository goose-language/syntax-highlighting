{
  "name": "goose-syntax-highlighting",
  "displayName": "Goose syntax highlighting",
  "description": "Official Goose language extension.",
  "version": "0.0.2",
  "engines": {
    "vscode": "^1.22.0"
  },
  "license": "MIT",
  "icon": "assets/logo.png",
  "publisher": "thomasvergne",
  "author": "thomasvergne",
  "repository": "https://github.com/goose-language/syntax-highlighting",
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "goose",
      "aliases": ["Goose", "goose"],
      "extensions": [".goose"],
      "configuration": "./language-configuration.json",
      "icon": {
        "dark": "./assets/icon.png",
        "light": "./assets/icon.png"
      }
    }],
    "grammars": [{
      "language": "goose",
      "scopeName": "source.goose",
      "path": "./syntaxes/goose.tmLanguage.json"
    }]
  }
}
